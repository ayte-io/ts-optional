{
  "compilerOptions": {
    "outDir": "dist",
    "esModuleInterop": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "removeComments": true,
    "strictNullChecks": true,
    "declaration": true,
    "target": "es5"
  },
  "include": ["src"]
}
